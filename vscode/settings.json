{
    "workbench.colorTheme": "Default Light+",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "none",
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'JetBrainsMonoNL NF', 'JetBrains Mono NL', 'Noto Sans JP', 'Noto Sans CJK JP', monospace",
    "editor.fontLigatures": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "files.eol": "\n",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "telemetry.telemetryLevel": "crash",
    "editor.selectionClipboard": false,
    "keyboard.dispatch": "keyCode",
    "[markdown]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[svelte]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "[astro]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[ruby]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "castwide.solargraph"
    },
    "solargraph.formatting": true,
    "[go]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": false,
    },
    "[yaml]": {
        "editor.tabSize": 2,
    },
    "[c]": {
        "editor.tabSize": 4
    },
    "[cpp]": {
        "editor.tabSize": 4
    },
    "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: LLVM, IndentWidth: 4, TabWidth: 4, UseTab: Never, IndentCaseLabels: true}",
    "[json]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.json-language-features",
    },
    "[jsonc]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[sql]": {
        "editor.tabSize": 4
    },
    "indentRainbow.excludedLanguages": [
        "plaintext",
        "sql"
    ],
    "path-intellisense.mappings": {
        "@": "${workspaceFolder}/src",
    },
    "javascript.suggest.paths": false,
    "typescript.suggest.paths": false,
    "gitlens.integrations.enabled": false,
    "git.confirmSync": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "prettier.printWidth": 120,
    "prettier.documentSelectors": [
        "**/*.astro"
    ],
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "vetur.format.enable": false,
    "vetur.validation.template": false,
    "svelte.enable-ts-plugin": false,
    "svelte.ask-to-enable-ts-plugin": false,
    "files.associations": {
        "*.njk": "html"
    },
}